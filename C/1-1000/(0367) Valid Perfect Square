bool isPerfectSquare(int num){
    int testnum = 1;
    int gain = 3;
    
    while (testnum < num)
    {
        if (testnum == 2147395600)
        {
            return 0;
        }
        testnum += gain;
        gain += 2;
    }
    
    if (testnum == num)
    {
        return 1;    
    }
    
    return 0;
}
